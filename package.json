{
  "name": "blog",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "typst:dev": "node scripts/typst-manager.ts dev",
    "typst:build": "node scripts/typst-manager.ts build",
    "setup:typst": "node scripts/setup-typst.js",
    "dev": "concurrently \"npm run typst:dev\" \"vite dev\"",
    "build": "npm run setup:typst && npm run typst:build && vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/adapter-vercel": "^6.2.0",
    "@sveltejs/enhanced-img": "^0.9.2",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^25.0.2",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "svelte": "^5.45.6",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  },
  "dependencies": {
    "@fontsource-variable/atkinson-hyperlegible-next": "^5.2.6",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "linkedom": "^0.18.12"
  }
}
